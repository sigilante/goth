{
  "category": "uncertainty",
  "description": "Uncertain value creation and error propagation",
  "tests": [
    {
      "name": "measure",
      "file": "examples/uncertainty/measure.goth",
      "description": "Create uncertain measurement value ± uncertainty",
      "cases": [
        {"input": [10.5, 0.3], "expected": "10.5±0.3"},
        {"input": [100.0, 5.0], "expected": "100±5"},
        {"input": [0.0, 0.1], "expected": "0±0.1"},
        {"input": [3.14159, 0.00001], "expected": "3.14159±0.00001"}
      ]
    }
  ]
}
